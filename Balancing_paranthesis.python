n = input()
s = []
c = 0
for i in n:
    c+=1
    if(i=='(' or i=='{' or i=='['):
        s.append(i)
    elif(len(s)>0):
        if(i==')' and s[-1]=='(' or i=='}' and s[-1]=='{' or i==']' and s[-1]=='['):
            s.pop()
        else:
            break
if(len(s)!=0):
    c = c + len(s)
print(c)
